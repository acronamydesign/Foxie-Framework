{
	"column":{
		"1":".col-1",
		"2":".col-2",
		"3":".col-3",
		"4":".col-4",
		"5":".col-5",
		"6":".col-6",
		"7":".col-7",
		"8":".col-8",
		"9":".col-9",
		"10":".col-10",
		"11":".col-11",
		"12":".col-12"
	},
	"navbar":{
		"all":"[class^=navbar]",
		"compact":".navbar--compact",
		"full":".navbar--full",
		"full-ctrl-right":".navbar--full.ctrl--right",
		"full-ctrl-left":".navbar--full.ctrl--left",
		"compact-ctrl-right":".navbar--compact.ctrl--right",
		"compact-ctrl-left":".navbar--compact.ctrl--left"
	},
	"logo":{
		"default":".logo",
		"light":".logo--light",
		"dark":".logo--dark",
		"negative":".logo--invert"
	},
	"container":{
		"default":".container",
		"fluid":".container--fluid"
	},
	"list":{
		"default":".list-group",
		"selectable":".list-group.--hover",
		"selectable-hairline":".list-group.--hover.--hairline"
	},
	"btn":{
		"default":".btn",
		"ghost":".btn--ghost",
		"invisible":".btn--invisible"
	},
	"inputs":{
		"default":"input",
		"search":".search"
	},
	"notify":{
		"stack":".stack",
		"item":".notify",
		"warning":".notify.warning",
		"info":".notify.info",
		"attention":".notify.attention",
		"success":".notify.success"
	},
	"hero":{
		"default":".hero"
	},
	"dropmenu":{
		"default":".navbar_dropdown",
		"right":".navbar_dropdown--right",
		"active":".navbar_dropdown--active"
	}
}